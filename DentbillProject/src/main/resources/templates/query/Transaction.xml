<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="templates.query.Transaction">
	
	<!-- 접수명단 등록-->
	<insert id="transactinoInsert" parameterType="Transaction">
		insert into dentbill_transaction
		(bill_id,reg_date,exp_date,clinic_id,patient_name,prosthetic_id,shade,amount,formula_id,note)
		values(bill_id_func(#{reg_date}),#{reg_date},#{exp_date},#{partner_id},#{patient_name},
		#{prosthetic_id},#{shade},#{amount},#{formula_id},#{note})
	</insert>

</mapper>